{
    "name": "canvass/canvass-core",
    "description": "A form building library",
    "type": "library",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "license": "MIT",
    "authors": [
        {
            "name": "Michael Kelly",
            "email": "puritandesigns@gmail.com"
        }
    ],
    "require": {
        "twig/twig": "^2.11",
        "illuminate/database": "^5.8",
        "doctrine/dbal": "^2.9",
        "illuminate/pagination": "^5.8",
        "illuminate/filesystem": "^5.8"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "phpunit/phpunit": "^8.3"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "Canvass\\": "src/Canvass"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/Tests"
        }
    }
}
