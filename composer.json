{
    "name": "canvass/canvass-core",
    "description": "A form building library",
    "type": "library",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "license": "MIT",
    "authors": [
        {
            "name": "Michael Kelly",
            "email": "puritandesigns@gmail.com"
        }
    ],
    "require": {
        "promo-code/web-anvil": "0.1-alpha"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "phpunit/phpunit": "^8.3"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "Canvass\\": "src/Canvass/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/Tests",
            "Implement\\": "tests/Implement/"
        }
    }
}
